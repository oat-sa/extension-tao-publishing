{

    "name": "oat-sa/extension-tao-publishing",
    "description" : "An extension to publish tests to a delivery environment",
    "type" : "tao-extension",
    "authors" : [
        {
        "name": "Open Assessment Technologies SA"
        }
    ],
    "keywords" : [
        "tao",
        "computer-based-assessment"
    ],
    "homepage" : "http://www.taotesting.com",
    "license" : [
        "GPL-2.0-only"
    ],
    "extra" : {
        "tao-extension-name" : "taoPublishing"
    },
    "minimum-stability" : "dev",
    "require": {
        "oat-sa/oatbox-extension-installer": "~1.1||dev-master",
        "psr/http-client": "^1",
        "oat-sa/generis": "^12.5",
        "guzzlehttp/psr7": "^1.4",
        "oat-sa/tao-core": "^v39.4"
    },
    "autoload" : {
        "psr-4" : {
            "oat\\taoPublishing\\" : ""
        }
    },
    "autoload-dev": {
        "psr-4": { "oat\\taoPublishing\\tests\\integration\\": "tests/integration/" }
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
